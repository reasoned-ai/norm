Article:: [String]
= 'the' | 'a'

Verb:: [String]
= 'see' | 'go', 'to'

Noun:: [String]
= 'New', 'York' | 'I'

Adj:: [String]
= 'beautiful'

Split::
partial: [String],
for i in range(len(partial)) (
 | -> (partial[:][:i], partial[:][i+1:])
)

VP::
partial: [String],
for left, right in Split(partial) (
  | -> Verb(left), NP(right)
)

NNP::
partial: [String],
for left, right in Split(partial) (
  | -> Adj(left), Noun(right)
)

NP::
partial: [String],
for left, right in Split(partial) (
  | -> Article(left), NNP(right)
)

Parse::
sentence: [String],
for left, right in Split(sentence) (
  | -> NP(left), VP(right)
)

Parse('I see the beautiful New York')
